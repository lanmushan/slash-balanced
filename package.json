{
  "name": "slash-balanced",
  "version": "1.0.0",
  "main": "dist/slash-balanced.umd.js",
  "module": "dist/slash-balanced.es.js",
  "typings": "dist/packages/index.d.ts",
  "description": "在特定场景下解决浏览器同源并发限制问题，提升网络加载性能",
  "repository": {
    "type ": "git",
    "url": "https://github.com/lanmushan/slash-balanced"
  },
  "homepage": "https://github.com/lanmushan/slash-balanced",
  "style": "dist/style.css",
  "keyword": [
    "同源限制",
    "HTTP请求",
    "HTTP1.1",
    "AJAX"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/slash-balanced.es.js",
      "require": "./dist/slash-balanced.umd.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*"
  ],
  "private": false,
  "scripts": {
    "serve": "vite",
    "demo": "vite build --config ./build/build.config.ts",
    "lib": "vite build --config ./build/lib.config.ts",
    "doc": "docsify serve docs",
    "preview": "vite preview"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.0",
    "axios": "^0.26.1",
    "element-plus": "^2.1.0",
    "highlight.js": "^11.5.0",
    "less": "4.1.3",
    "prismjs": "^1.27.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-dts": "^0.9.10",
    "vue": "^3.2.25",
    "vue-router": "^4.0.13",
    "vue-tsc": "^0.29.8"
  },
  "author": "一条摆摆鱼啊 <lanmu_shan@163.com>",
  "license": "MIT"
}
